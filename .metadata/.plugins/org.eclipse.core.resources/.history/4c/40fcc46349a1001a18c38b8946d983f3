package Cards;
import java.util.ArrayList;

public class Card {
	String suitOfCard;
	int number;
	
	public Card(String suit, int numberOfCard){
		this.suitOfCard = suit;
		this.number = numberOfCard;
	}

	public String toString() {
		String repr = this.number + " of " + this.suitOfCard;
		return repr;
	
	}
	
}
